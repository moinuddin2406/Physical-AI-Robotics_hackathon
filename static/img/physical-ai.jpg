<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400">
  <!-- Background -->
  <rect width="600" height="400" fill="#0a0f2c"/>
  
  <!-- Grid pattern -->
  <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
    <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#00bfff" stroke-width="0.5" opacity="0.2"/>
  </pattern>
  <rect width="600" height="400" fill="url(#grid)" />
  
  <!-- Central robot figure -->
  <g transform="translate(300, 150)">
    <rect x="-30" y="-40" width="60" height="80" rx="8" fill="#00bfff" opacity="0.8"/>
    <rect x="-15" y="-20" width="30" height="40" rx="4" fill="white" opacity="0.4"/>
    <rect x="-45" y="-20" width="8" height="40" rx="4" fill="#00bfff" opacity="0.8"/>
    <rect x="37" y="-20" width="8" height="40" rx="4" fill="#00bfff" opacity="0.8"/>
    <rect x="-28" y="40" width="12" height="50" rx="6" fill="#0077cc" opacity="0.8"/>
    <rect x="16" y="40" width="12" height="50" rx="6" fill="#0077cc" opacity="0.8"/>
    <circle cx="0" cy="-50" r="25" fill="#00bfff" opacity="0.8"/>
    <circle cx="-8" cy="-55" r="4" fill="white" opacity="0.9"/>
    <circle cx="8" cy="-55" r="4" fill="white" opacity="0.9"/>
    <circle cx="0" cy="-40" r="3" fill="#0077cc"/>
    
    <!-- Circuit pattern on body -->
    <path d="M-20,-10 Q0,-20 20,-10 T20,10 Q0,20 -20,10 T-20,-10" 
          fill="none" stroke="white" stroke-width="1" opacity="0.3"/>
  </g>
  
  <!-- Floating elements -->
  <circle cx="100" cy="100" r="6" fill="#00bfff" opacity="0.6"/>
  <circle cx="500" cy="80" r="4" fill="#00bfff" opacity="0.6"/>
  <circle cx="150" cy="300" r="5" fill="#00bfff" opacity="0.6"/>
  <circle cx="450" cy="320" r="3" fill="#00bfff" opacity="0.6"/>
  
  <!-- AI elements -->
  <g transform="translate(150, 250)">
    <circle cx="0" cy="0" r="20" fill="none" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
    <circle cx="0" cy="0" r="10" fill="none" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
    <line x1="-15" y1="0" x2="15" y2="0" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
    <line x1="0" y1="-15" x2="0" y2="15" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
  </g>
  
  <g transform="translate(450, 250)">
    <circle cx="0" cy="0" r="20" fill="none" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
    <circle cx="0" cy="0" r="10" fill="none" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
    <line x1="-15" y1="0" x2="15" y2="0" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
    <line x1="0" y1="-15" x2="0" y2="15" stroke="#00bfff" stroke-width="2" opacity="0.5"/>
  </g>
  
  <!-- Title -->
  <text x="300" y="370" text-anchor="middle" font-family="Arial" font-size="20" fill="white" opacity="0.9">Physical AI</text>
</svg>