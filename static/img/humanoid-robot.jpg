<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400">
  <!-- Background -->
  <rect width="600" height="400" fill="#0a0f2c"/>
  
  <!-- Grid pattern -->
  <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
    <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#00bfff" stroke-width="0.5" opacity="0.2"/>
  </pattern>
  <rect width="600" height="400" fill="url(#grid)" />
  
  <!-- Humanoid robot -->
  <g transform="translate(300, 150)">
    <!-- Body -->
    <rect x="-25" y="0" width="50" height="70" rx="8" fill="#00bfff" opacity="0.8"/>
    <!-- Chest panel -->
    <rect x="-15" y="10" width="30" height="25" rx="5" fill="white" opacity="0.3"/>
    <!-- Head -->
    <rect x="-20" y="-40" width="40" height="40" rx="8" fill="#00bfff" opacity="0.8"/>
    <!-- Face display -->
    <rect x="-15" y="-30" width="30" height="15" rx="3" fill="white" opacity="0.4"/>
    <!-- Eyes -->
    <circle cx="-8" cy="-32" r="3" fill="#0077cc"/>
    <circle cx="8" cy="-32" r="3" fill="#0077cc"/>
    <!-- Arms -->
    <rect x="-50" y="5" width="25" height="10" rx="5" fill="#00bfff" opacity="0.8"/>
    <rect x="25" y="5" width="25" height="10" rx="5" fill="#00bfff" opacity="0.8"/>
    <!-- Legs -->
    <rect x="-20" y="70" width="12" height="60" rx="6" fill="#0077cc" opacity="0.8"/>
    <rect x="8" y="70" width="12" height="60" rx="6" fill="#0077cc" opacity="0.8"/>
    <!-- Feet -->
    <rect x="-22" y="130" width="16" height="8" rx="4" fill="#005599" opacity="0.8"/>
    <rect x="6" y="130" width="16" height="8" rx="4" fill="#005599" opacity="0.8"/>
  </g>
  
  <!-- Circuit board pattern -->
  <g opacity="0.2">
    <path d="M100,300 Q150,280 200,300 T300,300 Q350,280 400,300 T500,300" 
          fill="none" stroke="#00bfff" stroke-width="1"/>
    <path d="M100,320 Q200,340 300,320 T500,320" 
          fill="none" stroke="#00bfff" stroke-width="1"/>
    <circle cx="150" cy="310" r="3" fill="none" stroke="#00bfff" stroke-width="1"/>
    <circle cx="250" cy="330" r="3" fill="none" stroke="#00bfff" stroke-width="1"/>
    <circle cx="350" cy="310" r="3" fill="none" stroke="#00bfff" stroke-width="1"/>
    <circle cx="450" cy="330" r="3" fill="none" stroke="#00bfff" stroke-width="1"/>
  </g>
  
  <!-- Floating elements -->
  <circle cx="100" cy="100" r="6" fill="#00bfff" opacity="0.6"/>
  <circle cx="500" cy="80" r="4" fill="#00bfff" opacity="0.6"/>
  <circle cx="150" cy="350" r="5" fill="#00bfff" opacity="0.6"/>
  <circle cx="450" cy="320" r="3" fill="#00bfff" opacity="0.6"/>
  
  <!-- Title -->
  <text x="300" y="370" text-anchor="middle" font-family="Arial" font-size="20" fill="white" opacity="0.9">Humanoid Robot</text>
</svg>